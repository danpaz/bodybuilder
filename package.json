{
  "name": "bodybuilder",
  "version": "1.4.0",
  "description": "An elasticsearch query body builder.",
  "main": "lib/index.js",
  "scripts": {
    "build": "babel src --out-dir lib",
    "build:browser": "webpack lib browser/bodybuilder.min.js",
    "check": "npm run lint && npm test",
    "docs": "documentation -o docs/API.md -f md src/index.js --shallow",
    "lint": "eslint src test",
    "test": "mocha --require babel-core/register --recursive test",
    "watch:build": "babel src --watch --out-dir lib"
  },
  "author": "Daniel Paz-Soldan <daniel.pazsoldan@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "babel-cli": "6.5.1",
    "babel-core": "6.5.1",
    "babel-plugin-lodash": "2.0.1",
    "babel-preset-es2015": "6.5.0",
    "chai": "3.2.0",
    "documentation": "3.0.4",
    "eslint": "1.10.2",
    "mocha": "2.4.5",
    "webpack": "1.12.13"
  },
  "dependencies": {
    "lodash": "4.9.0"
  },
  "repository": {
    "type": "git",
    "url": "http://github.com/danpaz/bodybuilder"
  },
  "keywords": [
    "elasticsearch",
    "bodybuilder",
    "querying",
    "queries",
    "query",
    "elastic",
    "search",
    "dsl"
  ]
}
